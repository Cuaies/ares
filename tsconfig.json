{
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "Node16",
    "target": "es2021",
    "rootDir": "./src/",
    "outDir": "./dist/",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
